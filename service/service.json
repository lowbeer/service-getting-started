{
  "memory": 128,
  "ttl": 5,
  "stack": "nodejs",
  "routes": {
    "getBanana": {
      "path": "/banana"
    },
    "createBanana": {
      "path": "/banana",
      "public": false
    },
    "apple": {
      "path": "/apple/:id",
      "public": true
    }
  },
  "events": {
    "onAppsEvent": {
      "sender": "apps"
    },
    "onBundleReady": {
      "sender": "vtex.render-builder",
      "keys": ["bundle-ready"]
    }
  }
}